var string_masoquismo = prompt("Digite aqui a sequência: ");
var array = string_masoquismo.split('');
var ultimo = (array.reverse()[0])
var x = prompt("Valor para S1->S2: ");
var y = prompt("Valor para S2->S1: ");
	
if (x == 0 && y == 1) {
	var maquina = "S1"
}

else if (x == 1 && y == 0){
		var maquina = "S2"
}
else{ 
	alert("impossivel");
}

alert("o ultimo elemento é: " + ultimo + " e a máquina de estados irá parar em: " + maquina);
